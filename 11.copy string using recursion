#include<stdio.h>
#include<string.h>
void copy(char[],char[]);
int main()
{
	char str1[20],str2[20];
	printf("enter string");
	scanf("%s",&str1);
	strcpy(str2, str1);
	printf("first string:%s\n",str1);
	printf("second string:%s",str2);
	return 0;
}
void copy(char str1[],char str2[])
{
    strcpy(str2, str1);
}
