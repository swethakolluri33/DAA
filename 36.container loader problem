#include<stdio.h>
int main()
{
	int c,i,n,a[100],e;
	printf("enter no of elements in container:\n");
	scanf("%d",&e);
	printf("enter elements:\n");
	for(i=0;i<e;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter capacity=\n");
	scanf("%d",&c);
	printf("enter a number");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		c=c-a[i];
		printf("%d is the left capacity\n",c);
	}
	return 0;
}
