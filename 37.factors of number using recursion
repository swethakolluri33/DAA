#include <stdio.h>
void printFactors(int number)
{
	int i;
    printf("\nFactors of %d are: ", number);
    for ( i = 1; i <= number; i++)
    {
        if (number % i == 0)
        {
            printf("%d ", i);
        }
    }
}
int main()
{
    int number;
    printf("Enter a number: ");
    scanf("%d", &number);
    printFactors(number); 
    return 0;
}
