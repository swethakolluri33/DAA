#include<stdio.h>
int main()
{
	int  size,i,large,arr[size];
	printf("enter size of array:");
	scanf("%d",&size);
	printf("enter %d elements",size);
	for(i=0;i<size;i++)
	{
		scanf("%d",&arr[i]);
	}
	large=arr[0];
	for(i=0;i<size;i++)
	{
		if(arr[i]<large)
		{
			large=arr[i];	
		}	
	}
	printf(" largest number=%d",large); 
}
